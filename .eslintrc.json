{
    "root": true,
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2017,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true,
            "impliedStrict": true
        },
        "sourceType": "module"
    },
    "plugins": [
      "eslint-plugin-flowtype"
    ],
    "rules": {
        "accessor-pairs": 2,
        "array-bracket-spacing": [1, "never"],
        "array-callback-return": 0,
        "arrow-body-style": 0,
        "arrow-parens": 0,
        "arrow-spacing": [1, { "before": true, "after": true }],
        "block-scoped-var": 0,
        "block-spacing": [1, "always"],
        "brace-style": [1, "1tbs", { "allowSingleLine": true }],
        "callback-return": 0,
        "camelcase": [1, { "properties": "always" }],
        "capitalized-comments": 0,
        "class-methods-use-this": 0,
        "comma-dangle": [1, "never"],
        "comma-spacing": [1, { "before": false, "after": true }],
        "comma-style": [1, "last"],
        "complexity": 0,
        "computed-property-spacing": [1, "never"],
        "constructor-super": 2,
        "consistent-return": 0,
        "consistent-this": [1, "self"],
        "curly": [1, "multi-line"],
        "default-case": 0,
        "dot-location": [1, "property"],
        "dot-notation": 1,
        "eol-last": [1, "always"],
        "eqeqeq": [1, "always", { "null": "ignore" }],
        "func-call-spacing": [1, "never"],
        "func-name-matching": 0,
        "func-names": 0,
        "func-style": [1, "expression", { "allowArrowFunctions": true }],
        "generator-star-spacing": [1, "after"],
        "global-require": 0,
        "guard-for-in": 0,
        "handle-callback-err": [1, "^(err|error)$"],
        "id-blacklist": 0,
        "id-length": [1, { "min": 0, "max": 32 }],
        "id-match": 0,
        "indent": [1, 4, {
            "SwitchCase": 1,
            "VariableDeclarator": 1,
            "MemberExpression": 1,
            "FunctionDeclaration": { "body": 1, "parameters": 2 },
            "FunctionExpression": { "body": 1, "parameters": 2 },
            "CallExpression": { "arguments": 1 },
            "ArrayExpression": 1,
            "ObjectExpression": 1
        }],
        "init-declarations": 0,
        "jsx-quotes": [1, "prefer-double"],
        "key-spacing": [1, { "beforeColon": false, "afterColon": true, "mode": "minimum"}],
        "keyword-spacing": [1, { "before": true, "after": true }],
        "line-comment-position": 0,
        "linebreak-style": [2, "unix"],
        "lines-around-comment": 0,
        "lines-around-directive": 0,
        "max-depth": [1, { "max": 6 }],
        "max-len": [1, {
            "code": 80,
            "ignoreStrings": true,
            "ignoreRegExpLiterals": true,
            "ignoreComments": true,
            "ignoreTemplateLiterals": true
        }],
        "max-lines": 0,
        "max-nested-callbacks": [1, { "max": 2 }],
        "max-params": [1, { "max": 16 }],
        "max-statements-per-line": [1, { "max": 1 }],
        "max-statements": 0,
        "multiline-ternary": 0,
        "new-cap": [1, { "newIsCap": true, "capIsNew": true, "properties": true }],
        "new-parens": 1,
        "newline-before-return": 0,
        "newline-per-chained-call": [1, { "ignoreChainWithDepth": 3 }],
        "no-alert": 1,
        "no-array-constructor": 2,
        "no-await-in-loop": 1,
        "no-bitwise": 0,
        "no-caller": 2,
        "no-case-declarations": 0,
        "no-catch-shadow": 0,
        "no-class-assign": 2,
        "no-cond-assign": [1, "except-parens"],
        "no-confusing-arrow": 0,
        "no-const-assign": 2,
        "no-constant-condition": [1, { "checkLoops": false }],
        "no-console": 0,
        "no-continue": 0,
        "no-control-regex": 0,
        "no-debugger": 1,
        "no-delete-var": 2,
        "no-div-regex": 0,
        "no-dupe-args": 2,
        "no-dupe-class-members": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-duplicate-imports": 2,
        "no-else-return": 0,
        "no-empty": 1,
        "no-empty-character-class": 1,
        "no-empty-function": 0,
        "no-empty-pattern": 1,
        "no-eq-null": 0,
        "no-eval": 2,
        "no-ex-assign": 1,
        "no-extend-native": 2,
        "no-extra-bind": 1,
        "no-extra-boolean-cast": 1,
        "no-extra-label": 1,
        "no-extra-semi": 1,
        "no-fallthrough": 1,
        "no-floating-decimal": 1,
        "no-func-assign": 1,
        "no-global-assign": 1,
        "no-implicit-coercion": 0,
        "np-implicit-globals": 0,
        "no-implied-eval": 2,
        "no-inline-comments": 0,
        "no-inner-declarations": [2, "functions"],
        "no-invalid-regexp": 2,
        "no-invalid-this": 1,
        "no-irregular-whitespace": 2,
        "no-iterator": 2,
        "no-label-var": 2,
        "no-labels": [1, { "allowLoop": false, "allowSwitch": false }],
        "no-lone-blocks": 1,
        "no-lonely-if": 1,
        "no-loop-func": 1,
        "no-magic-numbers": 0,
        "no-mixed-operators": [1, {
            "groups": [
                ["&", "|", "^", "~", "<<", ">>", ">>>"],
                ["==", "!=", "===", "!==", ">", ">=", "<", "<="],
                ["&&", "||"],
                ["in", "instanceof"]
            ],
            "allowSamePrecedence": true
        }],
        "no-mixed-requires": 1,
        "no-mixed-spaces-and-tabs": 1,
        "no-multi-spaces": 0,
        "no-multi-str": 1,
        "no-multiple-empty-lines": [1, { "max": 1 }],
        "no-negated-condition": 0,
        "no-nested-ternary": 0,
        "no-new": 1,
        "no-new-func": 2,
        "no-new-object": 1,
        "no-new-require": 1,
        "no-new-symbol": 1,
        "no-new-wrappers": 1,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-param-reassign": 0,
        "no-path-concat": 1,
        "no-plusplus": 0,
        "no-process-env": 0,
        "no-process-exit": 0,
        "no-proto": 1,
        "no-prototype-builtins": 0,
        "no-redeclare": 2,
        "no-regex-spaces": 1,
        "no-restricted-globals": 0,
        "no-restricted-imports": 0,
        "no-restricted-modules": 0,
        "no-restricted-properties": 0,
        "no-restricted-syntax": 0,
        "no-return-assign": [1, "except-parens"],
        "no-return-await": 1,
        "no-script-url": 2,
        "no-self-assign": 1,
        "no-self-compare": 1,
        "no-sequences": 2,
        "no-shadow": 0,
        "no-shadow-restricted-names": 2,
        "no-sparse-arrays": 2,
        "no-sync": 1,
        "no-tabs": 2,
        "no-template-curly-in-string": 1,
        "no-ternary": 0,
        "no-this-before-super": 2,
        "no-throw-literal": 1,
        "no-trailing-spaces": 1,
        "no-undef": 1,
        "no-undef-init": 1,
        "no-undefined": 0,
        "no-underscore-dangle": 0,
        "no-unexpected-multiline": 2,
        "no-unmodified-loop-condition": 2,
        "no-unneeded-ternary": [1, { "defaultAssignment": false }],
        "no-unreachable": 2,
        "no-unsafe-finally": 1,
        "no-unsafe-negation": 1,
        "no-unused-expressions": 0,
        "no-unused-labels": 1,
        "no-unused-vars": [1, { "vars": "all", "args": "none" }],
        "no-use-before-define": [1, { "functions": false, "classes": false }],
        "no-useless-call": 1,
        "no-useless-computed-key": 1,
        "no-useless-concat": 1,
        "no-useless-constructor": 1,
        "no-useless-escape": 1,
        "no-useless-rename": 1,
        "no-useless-return": 0,
        "no-var": 2,
        "no-void": 2,
        "no-warning-comments": 0,
        "no-whitespace-before-property": 1,
        "no-with": 2,
        "object-shorthand": [1, "methods"],
        "object-curly-newline": [0, { "multiline": true }],
        "object-curly-spacing": [1, "always", { "objectsInObjects": false }],
        "object-property-newline": [1, { "allowMultiplePropertiesPerLine": true }],
        "one-var-declaration-per-line": 0,
        "one-var": [1, { "initialized": "never" }],
        "operator-assignment": [1, "always"],
        "operator-linebreak": [1, "before", {
            "overrides": {
                "=": "after"
            }
        }],
        "padded-blocks": [1, "never"],
        "prefer-arrow-callback": 0,
        "prefer-const": 0,
        "prefer-numeric-literals": 1,
        "prefer-rest-params": 1,
        "prefer-spread": 1,
        "prefer-template": 0,
        "quote-props": 0,
        "quotes": [1, "single", { "avoidEscape": true, "allowTemplateLiterals": true }],
        "radix": [1, "as-needed"],
        "require-await": 1,
        "require-jsdoc": 0,
        "require-yield": 1,
        "rest-spread-spacing": [1, "never"],
        "semi": [1, "always", { "omitLastInOneLineBlock": true }],
        "semi-spacing": [1, { "before": false, "after": true }],
        "sort-keys": 0,
        "sort-imports": 0,
        "sort-vars": 0,
        "space-before-blocks": [1, "always"],
        "space-before-function-paren": [1, "always"],
        "space-in-parens": [1, "never"],
        "space-infix-ops": 1,
        "space-unary-ops": [1, { "words": true, "nonwords": false }],
        "spaced-comment": [1, "always"],
        "strict": 0,
        "symbol-description": 0,
        "template-curly-spacing": [1, "never"],
        "unicode-bom": [1, "never"],
        "use-isnan": 1,
        "valid-jsdoc": 0,
        "valid-typeof": 1,
        "vars-on-top": 0,
        "wrap-iife": [1, "any", { "functionPrototypeMethods": true }],
        "wrap-regex": 1,
        "yield-star-spacing": [1, "both"],
        "yoda": [1, "never"]
    }
}
